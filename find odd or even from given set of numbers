#include <stdio.h>

int main() {
    int arr[] = {3, 1, 4, 2, 7, 5, 3, 6, 4, 2}; // Example set of numbers
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Odd and even numbers in the array:\n");
    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) {
            printf("%d is even\n", arr[i]);
        } else {
            printf("%d is odd\n", arr[i]);
        }
    }

    return 0;
}
